{
  "name": "dynamic-redux-modules",
  "version": "1.0.0",
  "description": "Maintained clone of redux-dynamic-modules",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "dynamic",
    "redux",
    "modules"
  ],
  "author": "Microsoft & community devs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alex-oleksiiuk/dynamic-redux-modules/issues"
  },
  "homepage": "https://github.com/alex-oleksiiuk/dynamic-redux-modules#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist/**/*"
  ]
}
